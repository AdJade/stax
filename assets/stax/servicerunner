ssh -i ~/.ssh/STAX_KEY.pem -o "StrictHostKeyChecking no" core@`python -c "import random, sys; print random.choice(open(sys.argv[1]).readlines())," docker_hosts` 'fleetctl start ~/services/SOME_SERVICE.service'
