#scp -q -r -i ~/.ssh/STAX_KEY.pem -o "StrictHostKeyChecking no" services core@`python -c "import random, sys; print random.choice(open(sys.argv[1]).readlines())," docker_hosts`:~


runner=`cat docker_hosts|tail -n1`


scp -q -r -i ~/.ssh/vpc-stax-14618-menshevist.pem -o "StrictHostKeyChecking no" services core@${runner}:~

ssh -i ~/.ssh/vpc-stax-14618-menshevist.pem -o "StrictHostKeyChecking no" core@${runner} 'fleetctl submit ~/services/elasticsearch.service'

ssh -i ~/.ssh/vpc-stax-14618-menshevist.pem -o "StrictHostKeyChecking no" core@${runner} 'fleetctl start elasticsearch'
